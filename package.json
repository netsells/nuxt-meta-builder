{
  "name": "@netsells/nuxt-meta-builder",
  "version": "2.0.0",
  "description": "meta builder class so that you can build the meta object up in a more fluent way",
  "homepage": "https://github.com/netsells/nuxt-meta-builder",
  "author": {
    "name": "Martin Smith",
    "email": "martin.smith@netsells.co.uk"
  },
  "files": [
    "lib",
    "dist"
  ],
  "main": "lib/module.js",
  "keywords": [
    "nuxt-meta-builder",
    "vue",
    "nuxt"
  ],
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@netsells/eslint-config": "^2.3.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "eslint": "^5.9.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-require-jsdoc-except": "^1.3.0",
    "eslint-plugin-vue": "^5.1.0",
    "husky": "^1.2.0",
    "jest": "^23.5.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.2"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint ./lib/index.js",
    "test": "jest",
    "build": "babel -d ./dist ./lib/index.js",
    "prepublishOnly": "npm run build"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "license": "MIT",
  "browserslist": [
    "last 2 versions",
    "IE 11"
  ]
}
